  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:ID="communications">
    <rdfs:domain rdf:resource="#Proceedings"/>
    <rdfs:range rdf:resource="#InProceedings"/>
    <rdfs:label xml:lang="en">communications</rdfs:label>
